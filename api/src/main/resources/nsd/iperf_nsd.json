{  
    "name":"iperf-NSD",
    "vendor":"fokus",
    "version":"0.1-ALPHA",
    "vld":[  
        {  
            "name":"private"
        }
    ],
    "vnfd":[  
        {  
            "name":"iperf-server",
            "vendor":"fokus",
            "version":"0.2",
            "lifecycle_event":[  
                {  
                    "event":"INSTANTIATE",
                    "lifecycle_events":[  
                        "install.sh",
                        "install-srv.sh"
                    ]
                }
            ],
            "configurations":{  
                "configurationParameters":[  
                    {  
                        "confKey":"key",
                        "value":"value"
                    }
                ],
                "name":"server-configuration"
            },
            "virtual_link":[  
                {  
                    "name":"private"
                }
            ],
            "vdu":[  
                {  
                    "vm_image":[  
                        "ubuntu-14.04"
                    ],
                    "scale_in_out":2,
                    "vnfc":[  
                        {  
                            "connection_point":[  
                                {
                                    "floatingIp":"random",  
                                    "virtual_link_reference":"private"
                                }
                            ]
                        }
                    ],
                    "vimInstanceName":"vim-instance"
                }
            ],
            "deployment_flavour":[  
                {  
                    "flavour_key":"m1.small"
                }
            ],
            "type":"server",
            "endpoint":"generic",
            "vnfPackage":{
                "scriptsLink":"https://gitlab.fokus.fraunhofer.de/openbaton/scripts-test-public.git"
            }
        }
    ]
}
